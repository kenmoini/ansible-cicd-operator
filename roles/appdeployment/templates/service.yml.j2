---
apiVersion: v1
kind: Service
metadata:
  name: {{ ansible_operator_meta.name }}
  # namespace: {{ _cicd_kemo_dev_appdeployment_spec.target.namespace }}
  labels:
    app: {{ ansible_operator_meta.name }}
spec:
  ports:
    - name: 8080-tcp
      protocol: TCP
      port: 8080
      targetPort: 8080
  selector:
    app: {{ ansible_operator_meta.name }}
  type: ClusterIP
  sessionAffinity: None