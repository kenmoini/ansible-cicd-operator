---
apiVersion: v1
kind: Service
metadata:
  name: {{ _cicd_kemo_dev_appdeployment.metadata.name }}
  # namespace: {{ _cicd_kemo_dev_appdeployment.target.namespace }}
  labels:
    app: {{ _cicd_kemo_dev_appdeployment.metadata.name }}
spec:
  ports:
    - name: 8080-tcp
      protocol: TCP
      port: 8080
      targetPort: 8080
  selector:
    app: {{ _cicd_kemo_dev_appdeployment.metadata.name }}
  type: ClusterIP
  sessionAffinity: None